        <script rel="javascript">if(window.matchMedia&&window.matchMedia('(prefers-color-scheme: dark)').matches){console.log('Modo oscuro está habilitado.');cambiarModo('dark')}else{console.log('Modo oscuro no está habilitado.');cambiarModo('light')}function cambiarModo(modo){const body=document.body;if(modo==='dark'){body.setAttribute('data-color-mode','dark')}else{body.setAttribute('data-color-mode','light')}}</script>
